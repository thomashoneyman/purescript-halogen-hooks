{
  "name": "halogen-hooks",
  "private": true,
  "version": "0.0.1",
  "description": "Hooks for Halogen",
  "repository": "https://github.com/thomashoneyman/purescript-halogen-hooks",
  "author": "Thomas Honeyman <hello@thomashoneyman.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .spago output node_modules *.lock",
    "postinstall": "spago -x examples/examples.dhall install",
    "build": "spago build --no-install",
    "build:examples": "spago -x examples/examples.dhall build --no-install",
    "watch": "spago build --watch --no-install",
    "watch:examples": "spago -x examples/examples.dhall build --watch --no-install",
    "bundle": "spago -x examples/examples.dhall bundle-app --main Example.Main --to examples/app.js --no-install",
    "test:manual:isolated": "spago -x test/test.dhall bundle-app --main Test.Manual.Isolated --to test/app.js",
    "test:manual:shared": "spago -x test/test.dhall bundle-app --main Test.Manual.Shared --to test/app.js",
    "test:manual:conditional": "spago -x test/test.dhall bundle-app --main Test.Manual.Conditional --to test/app.js",
    "test:manual:number5": "spago -x test/test.dhall bundle-app --main Test.Manual.IsolatedTickEffect --to test/app.js"
  }
}
